<table class="table" id="compliance_dtable">
	<thead>
		<tr>
			<th colspan="2"></th>
			<th colspan="3"><center>Compliance</center></th>
			<th colspan="7" style="display:none;"></th>
		</tr>
		<tr>
			<th><center> Name </center></th>
			<th><center> Total</center> </th>
			<th><center> Monitoring</center> </th>
			<th><center> Reporting</center> </th>
			<th style="display:none;"> FP Compliance </th>
			<th style="display:none;"> Maternal Compliance </th>
			<th style="display:none;"> Health Houses Compliance </th>
			<th style="display:none;"> Support Group Compliance </th>
			<th style="display:none;"> Total Compliance Sorter </th>
			<th style="display:none;"> Monitoring Compliance Sorter </th>
			<th style="display:none;"> Reporting Compliance Sorter </th>
		</tr>
	</thead>
	<tbody>
		<% for unit in collection %>
			<tr id="<%= unit.name%>" class=<%=status(unit.total_percentage)%>>
				<% if unit.class.name=="Visitor" %>
					<td class="center"><%= link_to unit.name.titleize, send("compliance_report_user_path", unit), :class => "high-visibility" %> </td>
				<% else %>
					<td class="center"><%= link_to unit.name.titleize, send("compliance_report_#{unit.class.name.to_s.underscore}_path", unit), :class => "high-visibility" %> </td>
				<% end %>
				<td id="<%= unit.name%>" class="center <%=status(unit.total_percentage)%>"><%= number_to_percentage(unit.total_percentage, :precision => 1) %> </td>
				<td id="<%= unit.name%>" class="center"><%= number_to_percentage(unit.monitoring_percentage, :precision => 1) %></td>
				<td id="<%= unit.name%>" class="center"><%= number_to_percentage(unit.reporting_percentage, :precision => 1) %> </td>
				<td style="display:none;"><%= number_to_percentage(unit.fp_clients_percentage, :precision => 1) %>  (<%= unit.fp_clients_conducted.zero? ? (unit.fp_clients_conducted) : (link_to unit.fp_clients_conducted, phone_entries_path(:id=>unit, :resource_type => unit.class.name, :start_time=>@start_time, :end_time=>@end_time, :entry_type=>"Assessment"), :class => "high-visibility") %>/<%= unit.fp_clients_expected.to_s%>) </td>
				<td style="display:none;"><%= number_to_percentage(unit.maternals_percentage, :precision => 1) %>  (<%= unit.maternals_conducted.zero? ? (unit.maternals_conducted) : (link_to unit.maternals_conducted, phone_entries_path(:id=>unit, :resource_type => unit.class.name, :start_time=>@start_time, :end_time=>@end_time, :entry_type=>"Mentoring"), :class => "high-visibility") %>/<%= unit.maternals_expected.to_s%>) </td>
				<td style="display:none;"><%= number_to_percentage(unit.health_houses_percentage, :precision => 1) %>  (<%= unit.health_houses_conducted.zero? ? (unit.health_houses_conducted) : (link_to unit.health_houses_conducted, phone_entries_path(:id=>unit, :resource_type => unit.class.name, :start_time=>@start_time, :end_time=>@end_time, :entry_type=>"PdPst"), :class => "high-visibility") %>/<%= unit.health_houses_expected.to_s%>) </td>
				<td style="display:none;"><%= number_to_percentage(unit.support_group_meetings_percentage, :precision => 1) %>  (<%= unit.support_group_meetings_conducted.zero? ? (unit.support_group_meetings_conducted) : (link_to unit.support_group_meetings_conducted, phone_entries_path(:id=>unit, :resource_type => unit.class.name, :start_time=>@start_time, :end_time=>@end_time, :entry_type=>"PdDte"), :class => "high-visibility") %>/<%= unit.support_group_meetings_expected.to_s%>) </td>
				<td style="display:none;"><%= unit.total_percentage %> </td>
				<td style="display:none;"><%= unit.monitoring_percentage %> </td>
				<td style="display:none;"><%= unit.reporting_percentage %> </td>
			</tr>
		<% end %>
	</tbody>
</table>
<br/>
<br/>