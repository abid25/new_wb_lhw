
<% for indicator in @indicators %>
	
	loading_functions["Trend"]["<%=indicator.indicator_activity.name+"_"+indicator.hook%>"] = function()
	{
		<%= render_chart_with_transition indicator_graph([@facility,@district],indicator,'auto',550), 'visualization_area' %>
	}
	
<% end %>

loading_functions[current_visualization_type][current_visualization]();
$('#chart').html('<%= escape_javascript(render :partial => 'partials/indicators_table', :locals => { :collection => @facility, :indicators => @indicators }) %>');
apply_indicators_datatable();
