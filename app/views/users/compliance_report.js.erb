Gmaps.map.replaceMarkers(<%= raw generate_entry_markers(@phone_entries) %>);

loading_functions["total_compliance_trend"] = function()
{
	<%= render_chart_with_transition overall_compliance_trend([@officer,@district],'auto',550), 'visualization_area' %>
}

if (current_visualization_type!="map")
{
    loading_functions[current_visualization]();
	map_reload = 1;
}

toggle_markers('Assessment',document.getElementById('assessment_legend').checked);
toggle_markers('Mentoring',document.getElementById('mentoring_legend').checked);
toggle_markers('PdPst',document.getElementById('pdpst_legend').checked);
toggle_markers('PdDte',document.getElementById('pddte_legend').checked);

$('#chart').html('<%= escape_javascript(render '/phone_entries/entries_table') %>');

$(".poppable").popover(); //Always place this before applying datatables

apply_users_datatable();