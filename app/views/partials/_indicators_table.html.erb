<table class="table table-striped" id="indicators_dtable">
	<thead>
	<tr>
		<th colspan="1"></th>
		<th colspan="3"><center>Number of Students Appearing in Paper</center></th>
		<th colspan="1"></th>
		<th colspan="1"></th>
		<th colspan="4"><center>Score on Lesson Plan Indicator</center></th>
	</tr>
	<tr>
		<th><center>Name</center></th>
		<% for indicator in indicators %>
			<th><center><%=indicator.short_name%></center></th>
		<% end %>
	</tr>
			</thead>
			<tbody>
				<% for unit in collection %>
					<tr>
						<td class="center"><%= link_to unit.name.titleize, send("indicators_report_#{unit.class.name.to_s.underscore}_path", unit)%></td>
						<% for indicator in indicators %>
							<td class="center"> 
								<%= unit.get_indicator_value(indicator) %> 
							</td>
						<% end %>
					</tr>
				<% end %>
			</tbody>
</table>